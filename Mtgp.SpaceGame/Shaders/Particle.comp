struct Input
{
    [WorkgroupId] int workgroupId;
}

[Binding=0] uniform vec<int,2> particles;
[Binding=1] uniform vec<int,3> instances;

func void Main(Input input)
{
    var vec<int,2> particle;
    particle = particles[input.workgroupId];
    instances[input.workgroupId] = Vec(particle.x, 2, 3);
}